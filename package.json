{
    "name": "guardian-ai-crm",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc --noEmit --skipLibCheck && vite build",
        "build:fast": "vite build",
        "preview": "vite preview",
        "test": "vitest",
        "test:coverage": "vitest --coverage",
        "lint": "eslint . --max-warnings 350",
        "lint:fix": "eslint . --ext ts,tsx --fix",
        "lint:tsc": "tsc --noEmit",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "quality": "npm run lint:tsc && npm run lint && npm run format:check",
        "quality:fix": "npm run lint:fix && npm run format",
        "lint:role": "bash scripts/lint-api-role-usage.sh",
        "verify:role": "bash scripts/verify-api-role-usage.sh",
        "verify:jwt": "npx tsx scripts/verify-jwt-custom-claims.ts",
        "verify:all": "bash scripts/verify-role-cleanup.sh && bash scripts/verify-api-role-usage.sh && npm run verify:jwt",
        "prepare": "node scripts/setup-husky.js",
        "build:analyze": "npm run build && node scripts/build-analyzer.js",
        "build:production": "cross-env NODE_ENV=production npm run build",
        "performance:test": "npm run build && npm run build:analyze",
        "docs:update": "node scripts/generate-docs-report.js",
        "docs:manual": "bash scripts/manual-update.sh || .\\scripts\\manual-update.sh",
        "docs:check": "git diff MASTER_ROADMAP_OCT_2025.md TECHNICAL_ARCHITECTURE.md"
    },
    "dependencies": {
        "@dnd-kit/core": "^6.3.1",
        "@dnd-kit/sortable": "^10.0.0",
        "@dnd-kit/utilities": "^3.2.2",
        "@fullcalendar/core": "^6.1.19",
        "@fullcalendar/daygrid": "^6.1.19",
        "@fullcalendar/interaction": "^6.1.19",
        "@fullcalendar/react": "^6.1.19",
        "@fullcalendar/timegrid": "^6.1.19",
        "@google/genai": "^1.17.0",
        "@headlessui/react": "^2.2.9",
        "@heroicons/react": "^2.2.0",
        "@hookform/resolvers": "^5.2.2",
        "@supabase/supabase-js": "^2.75.0",
        "@types/file-saver": "^2.0.7",
        "@types/papaparse": "^5.3.16",
        "@types/qrcode": "^1.5.5",
        "@types/react-helmet": "^6.1.11",
        "@xyflow/react": "^12.8.6",
        "chart.js": "^4.5.1",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "dotenv": "^17.2.3",
        "esbuild": "0.21.5",
        "file-saver": "^2.0.5",
        "form-data": "^4.0.4",
        "jspdf": "^3.0.3",
        "jspdf-autotable": "^5.0.2",
        "lucide-react": "^0.544.0",
        "node-fetch": "^3.3.2",
        "papaparse": "^5.5.3",
        "pg": "^8.16.3",
        "qrcode": "^1.5.4",
        "react": "^19.1.1",
        "react-chartjs-2": "^5.3.0",
        "react-dom": "^19.1.1",
        "react-helmet-async": "^2.0.5",
        "react-hook-form": "^7.63.0",
        "react-hot-toast": "^2.6.0",
        "react-is": "^19.2.0",
        "react-router-dom": "^6.23.1",
        "recharts": "^3.3.0",
        "resend": "^6.1.3",
        "rrule": "^2.8.1",
        "tailwind-merge": "^3.3.1"
    },
    "devDependencies": {
        "@modelcontextprotocol/sdk": "^1.20.0",
        "@modelcontextprotocol/server-filesystem": "^2025.8.21",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.0.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/jest": "^30.0.0",
        "@types/node": "^20.19.22",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.1",
        "@types/react-router-dom": "^5.3.3",
        "@types/rrule": "^2.2.9",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^8.46.0",
        "@typescript-eslint/parser": "^8.46.0",
        "@vercel/node": "^5.4.0",
        "@vitejs/plugin-react": "^4.2.1",
        "autoprefixer": "^10.4.19",
        "cross-env": "^7.0.3",
        "eslint": "^8.57.1",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^6.1.1",
        "eslint-plugin-react-refresh": "^0.4.23",
        "eslint-plugin-security": "^3.0.1",
        "glob": "^10.3.10",
        "husky": "^9.1.7",
        "jest": "^30.2.0",
        "jsdom": "^24.1.3",
        "postcss": "^8.4.38",
        "prettier": "^3.6.2",
        "supabase": "^2.51.0",
        "tailwindcss": "^3.4.3",
        "terser": "^5.44.0",
        "tsx": "^4.20.6",
        "typescript": "^5.9.3",
        "vite": "^5.4.10",
        "vitest": "^3.2.4"
    },
    "engines": {
        "node": ">=18.20.0 <23",
        "npm": ">=9.0.0"
    },
    "packageManager": "npm@10.8.2",
    "resolutions": {
        "esbuild": "0.21.5"
    },
    "overrides": {
        "esbuild": "0.21.5"
    },
    "optionalDependencies": {
        "@rollup/rollup-win32-x64-msvc": "^4.52.5"
    }
}
